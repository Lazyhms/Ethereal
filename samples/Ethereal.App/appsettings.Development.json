{
  "Kestrel": {
    "Endpoints": {
      "Http": {
        "Url": "http://*:5300"
      }
    },
    "Limits": {
      "MaxConcurrentConnections": 100,
      "MaxConcurrentUpgradedConnections": 100
    }
  },
  "Serilog": {
    "Enrich": [ "FromLogContext", "WithMachineName", "WithThreadName" ],
    "Properties": {
      "Application": "Test1"
    },
    "MinimumLevel": {
      "Default": "Error",
      "Override": {
        "Default": "Error",
        "Microsoft": "Error",
        "Microsoft.Hosting.Lifetime": "Warning",
        "Microsoft.EntityFrameworkCore": "Error"
      }
    },
    "WriteTo": [
      {
        "Name": "Async",
        "Args": {
          "configure": [
            {
              "Name": "Console"
            },
            {
              "Name": "Debug"
            },
            {
              "Name": "File",
              "Args": {
                "path": "Logs/Error/log-.txt",
                "rollingInterval": "Day",
                "restrictedToMinimumLevel": "Error"
              }
            },
            {
              "Name": "File",
              "Args": {
                "path": "Logs/Information/log-.txt",
                "rollingInterval": "Day",
                "restrictedToMinimumLevel": "Information"
              }
            }
          ]
        }
      }
    ]
  },
  "SwaggerDoc": {
    "KOKO": {
      "Title": "mor1",
      "Description": "morrr1",
      "Version": "1.0"
    },
    "MOMO": {
      "Title": "mor",
      "Description": "morrr"
    }
  }
}
